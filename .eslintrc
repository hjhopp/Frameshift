{
    "extends": ["arenanet"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["svelte3"],
    "env": {
        "browser": true,
        "node": true
    },
    "overrides": [
        {
            "files": "*.svelte",
            "processor": "svelte3/svelte3"
        }
    ]
}
